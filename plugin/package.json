{
  "name": "hadocommun-plugin",
  "version": "1.0.0",
  "description": "Obsidian plugin for Hadocommun community",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "node --no-warnings esbuild.config.mjs production",
    "typecheck": "tsc -noEmit -skipLibCheck",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "generate-notices": "node scripts/generate-notices.js"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "hadocommun",
    "graph-view",
    "heading",
    "label"
  ],
  "author": "Hadocommun",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "0.17.3",
    "jest": "^30.2.0",
    "obsidian": "latest",
    "ts-jest": "^29.4.6",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}
